{
  "name": "amps-kerberos-authenticator",
  "version": "1.0.1",
  "description": "AMPS Kerberos Authenticator",
  "main": "amps-kerberos-authenticator.js",
  "types": "amps-kerberos-authenticator.d.ts",
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "amps": "5.3.0-0.333340.b0dae99",
